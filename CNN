    "CLASSES = [\"COVID\", \"Normal\", \"Viral Pneumonia\"]\n",
    "\n",
    "all_image_paths = []\n",
    "all_labels = []\n",
    "\n",
    "for class_idx, class_name in enumerate(CLASSES):\n",
    "    img_dir = DATA_DIR / class_name / \"images\"\n",
    "    class_images = list(img_dir.glob(\"*.png\"))\n",
    "    \n",
    "    all_image_paths.extend(class_images)\n",
    "    all_labels.extend([class_idx] * len(class_images))\n",
    "    \n",
    "    print(f\"{class_name:20s} : {len(class_images):5d} images (label={class_idx})\")\n",
    "\n",
    "full_dataset = COVIDDataset(all_image_paths, all_labels, transform=None)\n",
    "\n",
    "print(f\"\\nTotal images dans le dataset : {len(full_dataset)}\")\n",
    "\n",
    "print(\"\\nTest de chargement :\")\n",
    "for i in [0, 100, 3616, 3617, 5000, 10192, 10193, 13807, 13808, 13809]:\n",
    "    image, label = full_dataset[i]\n",
    "    print(f\"  Image {i:5d} : taille={image.size}, label={label} ({CLASSES[label]})\")\n"
   ]
