    "\n",
    "print(\"Dataset téléchargé (cache kagglehub) :\", downloaded_path)\n",
    "\n",
    "for item in downloaded_path.iterdir():\n",
    "    dest = MY_DATA_DIR / item.name\n",
    "    if item.is_dir():\n",
    "        if dest.exists():\n",
    "            shutil.rmtree(dest)\n",
    "        shutil.copytree(item, dest)\n",
    "    else:\n",
    "        shutil.copy2(item, dest)\n",
    "\n",
    "print(\"Dataset copié dans ton dossier :\", MY_DATA_DIR)\n",
    "print(\"Contenu (top-level) :\", [p.name for p in MY_DATA_DIR.iterdir()])\n",
