    "print(\"\\n\" + \"=\"*70)\n",
    "print(\"ÉVALUATION SUR LE TEST SET\")\n",
    "print(\"=\"*70)\n",
    "\n",
    "test_acc = evaluate_accuracy(model, test_loader, DEVICE)\n",
    "print(f\"Test Accuracy: {test_acc:.4f} ({test_acc*100:.2f}%)\")"
   ]
